{
  "Logging": {
    "Using":  ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Data.MCPImport.TSRG": "Information",
        "Microsoft" : "Warning",
        "Microsoft.EntityFrameworkCore" : "Warning",
        "System" : "Warning"
      },
      "Microsoft.EntityFrameworkCore" : "Warning"
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId", "WithExceptionDetails"]
  }
}
